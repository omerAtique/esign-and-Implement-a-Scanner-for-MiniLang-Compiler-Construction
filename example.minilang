
int x = 10;
int y = 20;
boolean isGreater = true;

int sum = x + y;

// Check conditions
if (sum >= 30) {
    print "Sum is greater than or equal to 30";
} else {
    print "Sum is less than 30";
}

// Update boolean variable
isGreater = sum > 30;

// Print the updated value
print "Is sum greater than 30? " + isGreater;
